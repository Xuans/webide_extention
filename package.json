{
    "name": "AgreeWebIDE",
    "displayName": "AgreeWebIDE",
    "description": "extention for agree",
    "version": "0.0.13",
    "publisher": "agree",
    "engines": {
        "vscode": "^1.28.2"
    },
    "categories": [
        "Other"
    ],
    "icon": "resources/icon.png",
    "activationEvents": [
        "onView:webide-config"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.webide.openDevTool",
                "title": "打开WEBIDE调试工具"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "webide-explorer",
                    "title": "WEBIDE项目视角",
                    "icon": "resources/aweb.svg"
                }
            ]
        },
        "views": {
            "webide-explorer": [
                {
                    "id": "webide-config",
                    "name": "配置"
                }
            ]
        }
    },
    "scripts": {
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.37.0",
        "eslint": "^5.13.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "compressing": "^1.4.0",
        "koa": "^2.7.0",
        "koa-body": "^4.1.1",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "koa-static-router": "^1.3.0",
        "mkdirp": "^0.5.1",
        "request": "^2.88.0",
        "rimraf": "^3.0.0",
        "urllib": "^2.34.0",
        "socket.io": "^2.5.0"
    }
}